import java.util.Arrays;

class Main {
    public static void main(String[] args) {
        int[] mass = {0, 1, 2, 3, 4, 5, 100};
        for (int i = 0; i < mass.length; i++) {
            for (int j = 0; j < mass.length - 1 - i; j++) {
                if(mass[j] >  mass[j + 1]) {
                    int buffer = mass[j];
                    mass[j] = mass[j + 1];
                    mass[j + 1] = buffer;
                }
            }

        }
        System.out.println(Arrays.toString(mass));
    }
}
