/*
* 3. Найти сумму квадратов первых ста чисел.
*/
package by.epam.lesson1;

class Main {
    public static void main(String[] args) {
        int val = 100;
        int result = 0;
        for(int i = 1; i <= val; i++){
            result += i * i;
        }
        System.out.println(result);
    }
}
