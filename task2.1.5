import java.util.ArrayList;

class Main {
    public static void main(String[] args) {
        int[] mass = {1, 2, 3, 4, 5, 6, 0, -10};
        ArrayList<Integer> newMass = new ArrayList<>();
        for (int i = 0; i < mass.length; i++) {
            if(mass[i] > i){
                newMass.add(mass[i]);
            }
        }
        System.out.println(newMass);
    }
}
