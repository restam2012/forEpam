class Main {
    public static void main(String[] args) {
        double[] mass = {1, 2, 3, 4, 5, 6, 0, -10};
        int positive = 0;
        int zero = 0;
        int negative = 0;
        for (double i:mass) {
            if(i > 0) positive++;
            else if(i == 0) zero++;
            else negative++;
        }
        System.out.println(positive + " " + zero + " " + negative);
    }
}
