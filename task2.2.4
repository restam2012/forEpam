class Main {
    public static void main(String[] args) {
        int n = 6;
        int[][] mass = new int[n][n];
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                if(i % 2 == 0){
                    mass[i][j] = j + 1;
                }
                else {
                    mass[i][j] = n - j;
                }
                System.out.print(mass[i][j] + " ");
            }
            System.out.println();
        }
    }
}
